{
    "dance.modes": {
        "normal": {
            "cursorStyle": "block",
            "selectionBehavior": "character",
        }
    },
    "debug.disassemblyView.showSourceCode": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "'Source Code Pro'",
    "editor.fontSize": 13,
    "editor.fontWeight": 600,
    "editor.guides.bracketPairs": true,
    "editor.selectionHighlight": false,
    "editor.semanticTokenColorCustomizations": {
        "[One Dark Pro Darker]": {
            "rules": {
                "variable": "#abb2bf",
                "property": "#abb2bf",
                "parameter": {
                    "foreground": "#abb2bf",
                    "italic": true
                },
                "*.mutable": {
                    "underline": false
                },
                "operator": "#e06c75",
                "punctuation": "#e06c75",
                "lifetime": "#56b6c2",
                "typeParameter": "#e06c75",
                "function.defaultLibrary": "#61afef",
                "builtinAttribute": "#61afef",
                "attribute": "#61afef"
            }
        }
    },
    "editor.smoothScrolling": true,
    "editor.tokenColorCustomizations": {
        "[One Dark Pro Darker]": {
            "variables": "#abb2bf",
            "textMateRules": [
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#e06c75"
                    }
                }
            ]
        }
    },
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "factorio.package.movedToCommitMessage": "Move to version $VERSION",
    "factorio.package.preparingCommitMessage": "Prepare to release version $VERSION",
    "factorio.package.tagName": "v$VERSION",
    "factorio.versions": [
        {
            "name": "Factorio",
            "factorioPath": "${env:FACTORIO}/bin/x64/factorio",
            "active": true
        },
        {
            "name": "Debug - 1.1",
            "factorioPath": "${env:FACTORIO_11}/bin/Debugx64GCC/factorio-run",
            "active": true
        },
        {
            "name": "Release - 1.1",
            "factorioPath": "${env:FACTORIO_11}/bin/Releasex64GCC/factorio-run",
            "active": true
        },
        {
            "name": "Release - 1.2",
            "factorioPath": "${env:FACTORIO_12}/bin/Releasex64GCC/factorio-run",
            "active": true
        }
    ],
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "http.systemCertificates": false,
    "keyboard.dispatch": "keyCode",
    "Lua.format.enable": false,
    "Lua.hover.expandAlias": false,
    "Lua.telemetry.enable": false,
    "[lua]": {
        "editor.defaultFormatter": "JohnnyMorganz.stylua",
        "editor.formatOnSave": true
    },
    "projectManager.git.baseFolders": [
        "~/dev"
    ],
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "stylua.disableVersionCheck": true,
    "stylua.styluaPath": "/home/rai/.cargo/bin/stylua",
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.fontWeight": 600,
    "update.mode": "none",
    "window.menuBarVisibility": "toggle",
    "workbench.colorCustomizations": {
        "[One Dark Pro Darker]": {
            "sideBar.background": "#181a1f",
            "editor.background": "#181a1f",
            "panel.background": "#181a1f",
            "activityBar.activeBackground": "#181a1f",
            "activityBar.background": "#181a1f",
            "terminal.background": "#181a1f"
        }
    },
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "git.openRepositoryInParentFolders": "never",
    "lldb.suppressUpdateNotifications": true,
    "clangd.arguments": [
        "--limit-references=0"
    ],
    "editor.inlayHints.enabled": "off",
    "workbench.editor.wrapTabs": true,
    "glassit.force_sway": true,
    "glassit.alpha": 220
}
