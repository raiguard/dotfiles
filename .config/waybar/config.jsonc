{
  "modules-left": [
    "sway/workspaces",
    "sway/mode",
  ],
  "modules-center": [
    "sway/window",
  ],
  "modules-right": [
    "cpu",
    "temperature",
    "memory",
    "battery",
    "network",
    "custom/weather",
    "custom/time",
    "tray",
  ],
  "cpu": {
    "format": "{usage}%",
    "interval": 2,
  },
  "temperature": {
    "hwmon-path": "/sys/class/hwmon/hwmon6/temp1_input",
    "interval": 2,
  },
  "memory": {
    "format": "{used} GB",
    "interval": 2,
  },
  "network": {
    "format-wifi": "{essid}",
  },
  "battery": {
    "states": {
      "critical": 20,
    },
    "format": "{capacity}%",
    "format-charging": "+{capacity}%",
    "format-plugged": "={capacity}%",
    "interval": 2,
  },
  "custom/weather": {
    "format": "{}Â°",
    "tooltip": true,
    "interval": 3600,
    "exec": "wttrbar --fahrenheit",
    "return-type": "json",
  },
  "custom/time": {
    "exec": "~/.config/waybar/time.sh",
    "interval": 60,
  },
  "tray": {
    "spacing": 3
  }
}
