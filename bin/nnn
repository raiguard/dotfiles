#!/bin/sh

prog=/usr/local/bin/nnn
if [ ! -f "$prog" ]; then
    prog=/usr/bin/nnn
fi

BLK="05" CHR="05" DIR="04" EXE="02" REG="00" HARDLINK="00" SYMLINK="06" MISSING="08" ORPHAN="01" FIFO="03" SOCK="03" OTHER="08"
NNN_FCOLORS="$BLK$CHR$DIR$EXE$REG$HARDLINK$SYMLINK$MISSING$ORPHAN$FIFO$SOCK$OTHER" $prog -A -H $@
